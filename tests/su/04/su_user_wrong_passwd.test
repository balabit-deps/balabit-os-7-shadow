#!/bin/sh

set -e

cd $(dirname $0)

. ../../common/config.sh
. ../../common/log.sh

log_start "$0" "su can be used to switch to a non-root user"

save_config

# restore the files on exit
trap 'log_status "$0" "FAILURE"; restore_config' 0

change_config

./run_su_failed.exp myuser myuserF00barbaz_wrongpass '$ '

log_status "$0" "SUCCESS"
restore_config
trap '' 0

